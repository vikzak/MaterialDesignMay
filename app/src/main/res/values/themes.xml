<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MaterialDesign" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="android:navigationBarColor">?superColor_500</item>
        <item name="colorPrimary">?superColor_500</item>
        <item name="colorOnPrimary">?superColor_500</item>
        <item name="colorPrimaryContainer">?superColor_100</item>
        <item name="colorOnPrimaryContainer">?superColor_900</item>
        <item name="colorPrimaryInverse">?superColor_200</item>
        <item name="colorPrimaryVariant">?superColor_700</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">?superColor_600</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="colorSecondaryContainer">?superColor_100</item>
        <item name="colorOnSecondaryContainer">?superColor_900</item>
        <item name="colorTertiary">?superColor_600</item>
        <item name="colorOnTertiary">@color/white</item>
        <item name="colorTertiaryContainer">?superColor_100</item>
        <item name="colorOnTertiaryContainer">?superColor_900</item>
        <item name="colorSecondaryVariant">?superColor_700</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/superColor_900</item>
        <!-- Customize your theme here. -->
        <item name="floatingActionButtonStyle">@style/myFloatingActionButtonStyle</item>
        <item name="bottomAppBarStyle">@style/myBottomAppBarStyle</item>
        <item name="textAppearanceHeadline1">@style/myHeadlineStyle</item>
    </style>

